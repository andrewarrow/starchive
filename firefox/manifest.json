{
  "name": "starchive",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "cookies"
  ],
  "host_permissions": [
    "http://localhost:3009/*",
    "https://*.youtube.com/*",
    "https://*.instagram.com/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Starchive",
    "default_icon": "star.png"
  },
  "icons": {
    "48": "star.png"
  }
}
